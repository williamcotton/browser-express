{
  "name": "browser-express",
  "version": "4.14.0",
  "description": "Fast, unopinionated, minimalist browser framework",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williamcotton/browser-express.git"
  },
  "keywords": [
    "browser",
    "express",
    "universal",
    "app",
    "framework"
  ],
  "author": "William Cotton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/williamcotton/browser-express/issues"
  },
  "homepage": "https://github.com/williamcotton/browser-express#readme",
  "dependencies": {
    "catch-links": "^2.0.1",
    "events": "^3.3.0",
    "form-serialize": "^0.7.2",
    "inherits": "^2.0.4",
    "qs": "^6.14.0",
    "router": "^2.0.0",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "verbose": true
  }
}
